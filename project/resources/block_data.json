{

    /* 
        
        [textures]
        - l: left (X-)
        - r: right (X+)
        - u: up (Y+)
        - d: down (Y-)
        - f: front (Z-)
        - b: back (Z+)
        
        [col_box] - Physical collision with entities
        
        [int_box] - 

    */

    "globals": {
        "col_box": {
            "cube": [1, 1, 1]
        },
        "int_box": {
            "cube": [1, 1, 1]
        }
    },

    "air": {"tangible": false, "col_box": null, "int_box": null},
    "grass": {"textures": {"u": [0, 0], "d": [2, 0], "e": [3, 0]}, "col_box": "$cube", "int_box": "$cube"},
    "stone": {"textures": [1, 0], "col_box": "$cube", "int_box": "$cube"},
    "dirt": {"textures": [2, 0], "col_box": "$cube", "int_box": "$cube"}

}